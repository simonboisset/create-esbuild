{
  "name": "create-esbuild-turbo",
  "version": "0.1.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=create-esbuild",
    "dev": "turbo run dev --filter=create-esbuild",
    "publish:major": "turbo run publish:major --filter=create-esbuild",
    "publish:minor": "turbo run publish:minor --filter=create-esbuild",
    "publish:patch": "turbo run publish:patch --filter=create-esbuild",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.5"
}
